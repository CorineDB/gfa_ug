                        ]
                    }
                }
            ],
            "statutCode": 200
        };

        const organisationSelect = document.getElementById('organisationSelect');
        const yearSelect = document.getElementById('yearSelect');
        const scoreTypeSelect = document.getElementById('scoreTypeSelect');
        const ctx = document.getElementById('governanceChart').getContext('2d');
        let governanceChart;

        // Fonction pour générer des couleurs uniques
        function getRandomColor() {
            const r = Math.floor(Math.random() * 255);
            const g = Math.floor(Math.random() * 255);
            const b = Math.floor(Math.random() * 255);
            return `rgba(${r}, ${g}, ${b}, 0.8)`;
        }

        // Fonction pour générer une couleur de fond avec transparence
        function getAreaColor(color) {
            return color.replace(/0\.8\)$/, '0.2)');
        }

        // Fonction pour initialiser le sélecteur d'organisation
        function populateOrganisationSelect() {
            organisationSelect.innerHTML = '';
            jsonData.data.forEach(org => {
                const option = document.createElement('option');
                option.value = org.id;
                option.textContent = org.intitule;
                organisationSelect.appendChild(option);
            });
        }

        // Fonction pour initialiser le sélecteur d'année
        function populateYearSelect(selectedOrganisationId) {
            const organisationData = jsonData.data.find(org => org.id === selectedOrganisationId);
            if (!organisationData) return;

            const years = Object.keys(organisationData.scores).sort();
            yearSelect.innerHTML = '<option value="all">Toutes les années</option>';
            
            years.forEach(year => {
                const option = document.createElement('option');
                option.value = year;
                option.textContent = year;
                yearSelect.appendChild(option);
            });
        }

        // Fonction pour préparer les données du graphique
        function prepareChartData(selectedOrganisationId, selectedScoreType, selectedYear) {
            const organisationData = jsonData.data.find(org => org.id === selectedOrganisationId);
            if (!organisationData) return { labels: [], datasets: [] };

            const labels = [];
            const principlesMap = new Map();

            // Déterminer les années à traiter
            const yearsToProcess = selectedYear === 'all' 
                ? Object.keys(organisationData.scores).sort() 
                : [selectedYear];

            // Parcourir les scores par année
            yearsToProcess.forEach(year => {
                const evaluations = organisationData.scores[year];
                if (!evaluations) return;
                
                evaluations.forEach(evaluation => {
                    labels.push(`${year} - ${evaluation.intitule}`);

                    const currentEvaluationPrinciples = new Set(evaluation.resultats.map(r => r.nom));

                    // Initialiser les principes existants
                    principlesMap.forEach((dataArray, principleName) => {
                        if (!currentEvaluationPrinciples.has(principleName) && dataArray.length < labels.length) {
                            dataArray.push(0);
                        }
                    });

                    // Ajouter les scores
                    evaluation.resultats.forEach(result => {
                        if (!principlesMap.has(result.nom)) {
                            principlesMap.set(result.nom, Array(labels.length - 1).fill(0));
                        }
                        principlesMap.get(result.nom).push(
                            result[selectedScoreType] !== undefined && result[selectedScoreType] !== null 
                                ? result[selectedScoreType] 
                                : 0
                        );
                    });

                    // Compléter avec des 0 si nécessaire
                    principlesMap.forEach((dataArray, principleName) => {
                        if (dataArray.length < labels.length) {
                            dataArray.push(0);
                        }
                    });
                });
            });

            const datasets = [];
            principlesMap.forEach((data, principleName) => {
                const color = getRandomColor();
                datasets.push({
                    label: principleName,
                    data: data,
                    borderColor: color,
                    backgroundColor: getAreaColor(color),
                    tension: 0.3,
                    fill: true,
                    pointBackgroundColor: 'white',
                    pointBorderWidth: 2,
                    pointRadius: 5,
                    pointHoverRadius: 7
                });
            });

            return { labels, datasets };
        }

        // Fonction pour obtenir le texte du type de score
        function getScoreTypeText(scoreTypeValue) {
            const options = Array.from(scoreTypeSelect.options);
            const selectedOption = options.find(option => option.value === scoreTypeValue);
            return selectedOption ? selectedOption.textContent : scoreTypeValue;
        }

        // Fonction pour mettre à jour le graphique
        function updateChart() {
            const selectedOrganisationId = organisationSelect.value;
            const selectedScoreType = scoreTypeSelect.value;
            const selectedYear = yearSelect.value;
            const chartData = prepareChartData(selectedOrganisationId, selectedScoreType, selectedYear);

            if (governanceChart) {
                governanceChart.destroy();
            }

            const yearText = selectedYear === 'all' ? 'Toutes les années' : selectedYear;
            const orgName = organisationSelect.options[organisationSelect.selectedIndex].text;

            governanceChart = new Chart(ctx, {
                type: 'line',
                data: {
                    labels: chartData.labels,
                    datasets: chartData.datasets
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        title: {
                            display: true,
                            text: `Scores ${getScoreTypeText(selectedScoreType)} - ${orgName} (${yearText})`,
                            font: {
                                size: 18
                            }
                        },
                        tooltip: {
                            mode: 'index',
                            intersect: false,
                        },
                        legend: {
                            display: true,
                            position: 'bottom'
                        }
                    },
                    scales: {
                        x: {
                            title: {
                                display: true,
                                text: 'Évaluations'
                            }
                        },
                        y: {
                            title: {
                                display: true,
                                text: `Score ${getScoreTypeText(selectedScoreType)}`
                            },
                            beginAtZero: true,
                            max: 1
                        }
                    }
                }
            });
        }

        // Événements
        organisationSelect.addEventListener('change', () => {
            populateYearSelect(organisationSelect.value);
            updateChart();
        });
        yearSelect.addEventListener('change', updateChart);
        scoreTypeSelect.addEventListener('change', updateChart);

        // Initialisation
        populateOrganisationSelect();
        if (organisationSelect.value) {
            populateYearSelect(organisationSelect.value);
            updateChart();
        } else {
            ctx.font = "20px Arial";
            ctx.textAlign = "center";
            ctx.fillText("Aucune donnée d'organisation disponible.", ctx.canvas.width/2, ctx.canvas.height/2);
        }
    </script>
